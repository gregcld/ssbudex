{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "prefetch",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani|json)"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "characters",
            "urls": [
                "./assets/db.json"
            ],
            "cacheConfig": {
                "maxSize": 5,
                "maxAge": "6h",
                "timeout": "10s",
                "strategy": "freshness"
            }
        }
    ]
}